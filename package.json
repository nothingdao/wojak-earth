{
  "name": "wojak-earth",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "prisma generate && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:seed": "tsx prisma/seed.ts",
    "setup:world": "tsx scripts/ultimate-world-setup.ts",
    "setup:clean": "tsx scripts/ultimate-world-setup.ts",
    "add:new-regions": "tsx scripts/add-new-regions.ts",
    "setup:new-regions": "tsx scripts/setup-new-regions-content.ts",
    "expand:world": "npm run add:new-regions && npm run setup:new-regions",
    "add:items": "tsx scripts/add-themed-items.ts",
    "add:tools": "tsx scripts/add-proper-tools.ts",
    "seed:mining": "tsx scripts/seed-mining-resources.ts",
    "seed:mining-tools": "tsx scripts/update-mining-with-tools.ts",
    "seed:markets": "tsx scripts/seed-enhanced-markets.ts",
    "debug:markets": "tsx scripts/debug-market-locations.ts",
    "fix:markets": "tsx scripts/fix-market-locations.ts",
    "setup:unified": "tsx scripts/setup-complete-game.ts",
    "setup:complete": "npm run add:items && npm run add:tools && npm run seed:mining && npm run seed:mining-tools && npm run fix:markets && npm run seed:markets",
    "setup:quick": "npm run fix:markets && npm run seed:markets",
    "functions:dev": "netlify dev",
    "functions:build": "netlify build",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@solana/wallet-adapter-base": "^0.9.26",
    "@solana/wallet-adapter-react": "^0.15.38",
    "@solana/wallet-adapter-wallets": "^0.19.36",
    "@solana/web3.js": "^1.98.2",
    "@tailwindcss/vite": "^4.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "prisma": "^6.8.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8"
  },
